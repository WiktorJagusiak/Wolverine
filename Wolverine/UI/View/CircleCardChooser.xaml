<UserControl x:Class="Wolverine.UI.Control.CircleCardChooser"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Wolverine.UI.Control"
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="200"
             d:DataContext="CircleCardChooserViewModel.cs">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="{Binding CurrentCard}" VerticalAlignment="Top" Width="120"/>
    </Grid>
</UserControl>
